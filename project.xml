<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="RuneGenerator" package="com.watabou.runegenerator" version="1.0.0" company="Retronic Games" />
	<app main="com.watabou.runegenerator.Main" path="Export" file="RuneGenerator" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="msignal" />

	<!-- General -->
	<window background="0xcccac2" fps="60" vsync="true" hardware="true" allow-high-dpi="true"/>
	<!-- Desktop -->
	<window width="640" height="640" fullscreen="false" resizable="true" borderless="false" if="desktop"/>
	<!-- Mobile -->
	<window width="0" height="0" fullscreen="true" orientation="landscape" if="mobile" />
	
	<assets path="Assets">
		<image path="table.png" id="table" />
		<image path="alphabet.png" id="alphabet" />
	</assets>
	
</project>